    <!-- morris JavaScript -->
    <script>
        $(document).ready(function () {
            //BOX BUTTON SHOW AND CLOSE
            jQuery('.small-graph-box').hover(function () {
                jQuery(this).find('.box-button').fadeIn('fast');
            }, function () {
                jQuery(this).find('.box-button').fadeOut('fast');
            });
            jQuery('.small-graph-box .box-close').click(function () {
                jQuery(this).closest('.small-graph-box').fadeOut(200);
                return false;
            });

            //CHARTS
            function gd(year, day, month) {
                return new Date(year, month - 1, day).getTime();
            }

            graphArea2 = Morris.Area({
                element: 'hero-area',
                padding: 10,
                behaveLikeLine: true,
                gridEnabled: false,
                gridLineColor: '#dddddd',
                axes: true,
                resize: true,
                smooth: true,
                pointSize: 0,
                lineWidth: 0,
                fillOpacity: 0.85,
                data: [
                    { month: '2019-01', booking: 2200, customer: 2668, visitor: 2649 },
                    { month: '2019-02', booking: 1200, customer: 200, visitor: 2649 },
                    { month: '2019-03', booking: 1300, customer: 1300, visitor: 1300 },
                    { month: '2019-04', booking: 200, customer: 2668, visitor: 2649 },
                    { month: '2019-05', booking: 2600, customer: 2668, visitor: 2649 },
                    { month: '2019-06', booking: 2600, customer: 2668, visitor: 1300 },
                    { month: '2019-07', booking: 300, customer: 1300, visitor: 2649 },
                    { month: '2019-08', booking: 2668, customer: 2668, visitor: 2649 },
                    { month: '2019-09', booking: 200, customer: 200, visitor: 200 },

                ],
                lineColors: ['#a44e4e', '#926383', '#eb6f6f '],
                xkey: 'month',
                redraw: true,
                ykeys: ['booking', 'customer', 'visitor'],
                labels: ['Booking Total', 'Customer Total', 'Visitors Total'],
                pointSize: 2,
                hideHover: 'auto',
                resize: true
            });


        });
    </script>
