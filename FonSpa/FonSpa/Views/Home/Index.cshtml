                            <img src="/Asset/Client/images/icons/logo-01.png" alt="IMG">
                        </a>
                    </div>

                    @{Html.RenderAction("MainMenu", "Home");}
                </div>

                <div class="group-feature right-side">
                    <div class="hun-element-search layout-1">
                        <span class="search-open">
                            <i class="zmdi zmdi-search"></i>
                        </span>

                        <div class="search-form">
                            <span class="search-close"></span>

                            <form>
                                <input type="text" class="search-field" />
                                <span class="search-notice"> Hit enter to search or ESC to close</span>
                            </form>

                            <ul class="list-result"></ul>
                        </div>
                    </div>

                    <div class="hun-menu-mobile">
                        <a href="#" class="btn-toggle-menu"></a>

                        <nav class="mobile-navigation">
                            <div class="logo-menu-mobile text-center">
                                <a href="#">
                                    <img src="/Asset/Client/images/icons/logo-footer-01.png" alt="IMG">
                                </a>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- end Header -->
    <!-- slider home-1 -->
    <div class="sec--main-slider--home-1">
        <div class="hun-element-main-slider layout-1">
            <div class="main-slider js-call-slick" data-animate="true">
                <div class="slide-slick" data-slick='{"fade": true, "speed": 900, "dots": true, "arrows": false, "autoplay": true, "autoplaySpeed": 4500, "infinite": true, "slidesToShow": 1, "slidesToScroll": 1}'>
                    @foreach (var slide in listSlide)
                    {
                        <div class="item-slick full-height-with-header" style="background-image: url('@slide.image');">
                            <div class="content-slide">
                                <div class="subtitle-slide" data-appear="slideInDown" data-delay="800">
                                    @slide.subtitle
                                </div>

                                <h2 class="title-slide" data-appear="slideInUp" data-delay="0">
                                    @slide.title
                                </h2>

                                <div class="buttons-slide" data-appear="zoomIn" data-delay="1600">
                                    <a href="@slide.link" class="hun-button size-normal style-border-light shape-round">
                                        @slide.TextButton
                                    </a>
                                </div>
                            </div>
                        </div>
                    }



                </div>

                <div class="dots-slick"></div>
            </div>

            <div class="social-slider">
                @foreach (var footer in footers)
                {
                    foreach (var category in footerCategories)
                    {
                        if (category.name == "Social")
                        {
                            if (category.id == footer.typeId)
                            {
                                <a href="@footer.link" class="item-social">
                                    <i class="@footer.Icon"></i>
                                </a>
                            }
                        }
                    }
                }

            </div>
        </div>
    </div>
