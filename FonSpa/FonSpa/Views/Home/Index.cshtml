    <!-- book-service home-1 -->
    <div class="sec--book-service--home-1 parallax100" style="background-image: url('/Asset/Client/images/bg-03.jpg');">
        <div class="container">
            <div class="row no-gutters justify-content-end">
                <div class="col-lg-6">

                    @using (Html.BeginForm("CreateBooking", "Booking", FormMethod.Post, new { @class = "book-service-form-01 no-border" }))
                    {
                        <div class="heading-form">
                            <h3 class="title-form">
                                Book services
                            </h3>

                            <img class="line-heading" src="~/Asset/Client/images/icons/wave-line-03.png" alt="IMG">
                        </div>

                        <label class="wrap-input">
                            <input required id="name" name="name" type="text" placeholder="Your full name *">

                            <i class="icon-user"></i>
                        </label>

                        <label class="wrap-input">
                            <input required id="phone" name="phone" type="text" pattern="(09|01[2|6|8|9])+([0-9]{8})\b" placeholder="Your phone number *">

                            <i class="icon-call-out"></i>
                        </label>

                        <label class="wrap-input js-call-select2" data-style="layout-1">
                            <select id="IdServices" name="IdServices">
                                <option value="0">Services *</option>
                                @foreach (var service in servicesList)
                                {
                                    <option value="@service.id">@service.name</option>
                                }

                            </select>

                            <i class="lnr lnr-chevron-down"></i>
                        </label>



                        <label class="wrap-input js-call-datepicker" data-drop="down" data-drop-inside="true">
                            <input id="ArrivalTime" type="text" name="ArrivalTime" autocomplete="off">

                            <i class="lnr lnr-chevron-down"></i>
                        </label>

                        <label class="wrap-input js-call-select2" data-style="layout-1">
                            <select id="time" name="time">
                                <option value="8">8:00 AM</option>
                                <option value="9">9:00 AM</option>
                                <option value="10">10:00 AM</option>
                                <option value="11">11:00 AM</option>
                                <option value="13">1:00 PM</option>
                                <option value="14">2:00 PM</option>
                                <option value="15">3:00 PM</option>
                                <option value="16">4:00 PM</option>
                                <option value="17">5:00 PM</option>
                                <option value="18">6:00 PM</option>
                            </select>

                            <i class="icon-clock"></i>
                        </label>

                        <label class="wrap-input js-call-select2" data-style="layout-1">
                            <select id="IdBed" name="IdBed">
                                <option value="0">Select Spa Room-Bed *</option>
                                @foreach (var room in roomsList)
                                {
                                    foreach (var bed in bedsList)
                                    {
                                        if (bed.IdRoom == room.Id)
                                        {
                                            <option value="@bed.Id">@room.Name : @bed.Name</option>
                                        }

                                    }

                                }
                            </select>

                            <i class="lnr lnr-chevron-down"></i>
                        </label>

                        <button class="hun-button size-normal shape-round style-fill-color">
                            Make appointment
                        </button>
                    }
                </div>
            </div>
        </div>
    </div>
